{
  "name": "firstproject",
  "jest": {
    "verbose": true,
    "rootDir": "src"
  },
  "version": "1.0.0",
  "description": "My first project",
  "main": "src/index.ts",
  "scripts": {
    "test": "NODE_ENV=test jest --coverage",
    "start": "NODE_ENV=development nodemon src/index.ts",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "local:dev:typeorm": "npm run typeorm:dev",
    "typeorm:dev": "NODE_ENV=development ts-node ./node_modules/.bin/typeorm --config ormconfig.ts",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm -- migration:generate --config ormconfig.ts --connection  --name ",
    "migration:run": "npm run typeorm -- migration:run",
    "seeder:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seeder:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yevhenko/FirstProject.git"
  },
  "author": "Yevhen Svyrydov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yevhenko/FirstProject/issues"
  },
  "homepage": "https://github.com/Yevhenko/FirstProject#readme",
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "ioredis": "^4.28.2",
    "pg": "^8.6.0",
    "redis": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "ts-dotenv": "^0.8.3",
    "typeorm": "^0.2.32",
    "zod": "^3.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "0.0.17",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.5.6",
    "@types/ioredis": "^4.28.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "jest": "^27.0.4",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
